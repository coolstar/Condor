{
  "name": "TweetDeck Enhancer",
  "short_name": "TD Enhancer",

  "description": "__MSG_ExtDesc__",
  "version": "5.0.4",

  "default_locale": "en_US",

  "permissions": [
    "https://tweetdeck.twitter.com/"
  ],

  "content_scripts": [
    {
      "matches": [ "https://tweetdeck.twitter.com/" ],
      "js": [ "content.js" ],
      "run_at": "document_end"
    }
  ],

  "icons": {
    "16": "resources/AppIcon16.png",
    "24": "resources/AppIcon24.png",
    "32": "resources/AppIcon32.png",
    "48": "resources/AppIcon48.png",
    "64": "resources/AppIcon64.png",
    "128": "resources/AppIcon128.png",
    "256": "resources/AppIcon256.png"
  },

  "web_accessible_resources": [
    "resources/*"
  ],

  "manifest_version": 2
}
